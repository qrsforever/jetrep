{
    "GstPipelineApp": {
        "log_level": 10,
        "log_datefmt": "%Y-%m-%d %H:%M:%S",
        "log_format": "%(asctime)s %(funcName)s:%(lineno)d %(name)s %(levelname)s %(message)s",
        "camera": "USB",
	    "rtmpsink": false,
        "filesink": false
    },
    "USBCamera": {
        "number_buffers": -1,
        "width": 640,
        "height": 480,
        "framerate": 30,
        "flip_method": 0
    },
    "CSICamera": {
        "width": 640,
        "height": 480,
        "framerate": 30,
        "flip_method": 0
    },
    "GH264CodecCvt": {
        "bitrate": 500
    },
    "ShareMemorySink": {
        "path": "/tmp/gst_repnet.shm",
        "size": 10000000
    },
    "MultiFilesSink": {
        "location": "/srs/gst",
        "maxfiles": 20,
        "duration": 300
    },
    "SRSRtmpSink": {
        "server": "0.0.0.0",
        "port": 1935,
        "stream": "1",
        "duration": 600
    }
}

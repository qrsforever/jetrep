[Unit]
    Description=JetRep SOS Service
    Documentation=http://jetrep.hzcsai.com
    After=multi-user.target

[Service]
    Type=oneshot
    User=root
    Group=root
    UMask=0000
    EnvironmentFile=/jetrep/etc/jetrep.env
    ExecStart=/jetrep/scripts/jetrep_recovery.sh

[Install]
    WantedBy=multi-user.target
